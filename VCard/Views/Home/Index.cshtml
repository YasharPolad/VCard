@model List<Vcard>

@{
    ViewData["Title"] = "Home Page";
}

<div class="row">

@foreach(Vcard vcard in Model)
{    
    <div class="card col-md-5 col-lg-3" style="width: 18rem;">
        @Html.Raw(vcard.QRImageString)
        <div class="card-body">
        <h5 class="card-title">@vcard.Firstname @vcard.Surname</h5>
        <p class="card-text">@vcard.Country @vcard.City</p>
        <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
    </div>    
}
</div>
